<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="MemberUserProductPurchase">
	
	<groups>
		<run>
			<include name="memberuser"/>
		</run>
	</groups>
	
	<listeners>
		<listener class-name="com.nopcommerce.listeners.MyListerners" />
	</listeners>
	
	<test name="MemberUserProductPurchase">
		<classes>
      		<class name="com.nopcommerce.testcases.RegisterTest"/>
      		
      		<class name="com.nopcommerce.testcases.LoginTest"/>
      		
      		<class name="com.nopcommerce.testcases.MyAccountTest"/>
      		
      		<class name="com.nopcommerce.testcases.ProductPurchaseTest"/>
      		
     	</classes>

	</test> <!-- Test -->
</suite> <!-- Suite -->
